    "a=int(input(\"enter a number\"))\n",
    "n=len(str(a))\n",
    "s=0\n",
    "temp=a\n",
    "d=0\n",
    "while temp>0:\n",
    "    d=temp%10\n",
    "    s=s+d**n\n",
    "    temp=temp//10\n",
    "if(s==a):\n",
    "    print(\"armstrong\")\n",
    "else:\n",
    "    print(\"not armstrong\")"
